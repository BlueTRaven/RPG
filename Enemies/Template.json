{	//Please note that these comments are here for your convienence; they will cause parsing errors if used in code, so copy from below.
	"id": "unique_id_goes_here",	//Throws exception if id is already used
	"name": "name_goes_here",
	"health": 100,
	
	"attacks": [
		{	//an array of attacks that the enemy will use. occurance is the chance the enemy will use that attack.
			"occurance": 1,	//default
			"dealDamage": {
				"damageType": 0,	//physical	
				"percent": 1	//default
			}
		}
	],
	
	"resistances": [
		{
			"damageType": 1,	//fire
			"percent": .5	//50% resistance to fire
		}
	]
}

